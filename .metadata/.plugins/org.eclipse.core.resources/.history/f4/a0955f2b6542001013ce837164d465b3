#include<stdint.h>
#include "uart0.h"
#include "adc0-1.h"
int main(void)
{
UART0_Init();
ADC0_Init();
ADC1_Init();
ADC0_StartConv();
ADC1_StartConv();
uint32_t x=ADC0_Read();
uint32_t y=ADC1_Read();
UART0_SendString("passenger seat temperature:- ");
UART0_SendNumber(x);
UART0_SendString("driver seat temperature:- ");
UART0_SendNumber(y);
while(1)
{

}
}
